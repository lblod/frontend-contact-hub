<AuHelpText @error={{true}}>
  {{@error.message}}
  {{#if @error.context.administrativeUnit}}
  <AuLink
    @route="administrative-units.administrative-unit"
    @model={{@error.context.administrativeUnit.id}}
    target="_blank"
  >
    {{@error.context.administrativeUnit.name}}
  </AuLink>
  {{/if}}
</AuHelpText>